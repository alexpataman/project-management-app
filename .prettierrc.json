{
  "endOfLine": "auto",
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "printWidth": 100,
  "arrowParens": "always",
  "importOrder": [
    "^@core/(.*)$",
    "^@server/(.*)$",
    "^@ui/(.*)$",
    "^[./](?!.*?\\.scss$).*$",
    "(.*)scss$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
